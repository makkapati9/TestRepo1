<!DOCTYPE html>
<html>
<head>
<title>Seller Login</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
<link href="style/style9.css" rel='stylesheet' type='text/css' />
<!-- fonts -->
<!-- fonts -->
</head>
<body>
	<!-- login-form-->
	<div class="login-form">
		<h1>Seller Login Form</h1>
		<!--form-left-->
		<div class="form-left">
			<form name="sellerform" id="sellerform" method="post">
				<div class="email">
					<span>Email:</span>
					<input type="text" class="form-control" id="name" name="name" value="Your Email here" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Your Email here';}">
					<div class="clear"> </div>
				</div>
				<div class="password">
					<span>Password:</span>
					<input type="password" class="form-control" id="pass" name="pass" value="123456789" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '123456789';}">
					<div class="clear"> </div>
				</div>
			
				<input type="button" value="" />
			</form>
		</div>
		<!--form-left-->
		<!--copy-right-->
		<div class="copy-right">
			<p>Design By <a href="http://w3layouts.com/">W3layouts</a></p>
		</div>
		<div class="clear"> </div>
		<!--copy-right-->
	</div>
	<!-- login-form-->
</body>
</html>